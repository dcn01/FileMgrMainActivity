<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_pressed="true">
        <bitmap android:src="@drawable/hw_ic_clear_pressed" android:tint="@color/hw_color_primary_dark" />
    </item>
    <item android:state_enabled="true">
        <bitmap android:src="@drawable/hw_ic_clear_normal"  android:tint="@color/hw_color_primary" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/hw_ic_clear_disabled" android:tint="@color/hw_color_primary_disabled" />
    </item>
    <item>
        <bitmap android:src="@drawable/hw_ic_clear_normal"  android:tint="@color/hw_color_primary"/>
    </item>

</selector>