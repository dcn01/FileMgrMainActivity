<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed = "false" >
        <bitmap android:src="@drawable/ic_done_normal" android:tint="@color/back_btn_color_disable" />
    </item>
    <item android:state_enabled="true" android:state_pressed= "true">
        <bitmap android:src="@drawable/ic_done_pressed" android:tint="@color/back_btn_color_dark" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/ic_done_disable" android:tint="@color/back_btn_color" />
    </item>
</selector>