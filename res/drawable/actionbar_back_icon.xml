<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_enabled="false">
    <bitmap android:src="@drawable/actionbar_back_disabled" android:tint="@color/back_btn_color_disable" />
</item>
<item android:state_pressed="true">
    <bitmap android:src="@drawable/actionbar_back_pressed" android:tint="@color/back_btn_color_dark" />
</item>
<item android:state_focused="true">
    <bitmap android:src="@drawable/actionbar_back_normal" android:tint="@color/back_btn_color" />
</item>

<item><bitmap android:src="@drawable/actionbar_back_normal" android:tint="@color/back_btn_color" />
</item>

</selector>